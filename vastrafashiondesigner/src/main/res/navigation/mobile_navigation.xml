<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_catalogues">

    <fragment
        android:id="@+id/navigation_catalogues"
        android:name="com.esolution.vastrafashiondesigner.ui.products.CataloguesFragment"
        android:label="@string/title_catalogues"
        tools:layout="@layout/fragment_catalogues" />

    <fragment
        android:id="@+id/navigation_orders"
        android:name="com.esolution.vastrafashiondesigner.ui.orders.OrdersFragment"
        android:label="@string/title_orders"
        tools:layout="@layout/fragment_orders" />

    <fragment
        android:id="@+id/navigation_chat"
        android:name="com.esolution.vastrafashiondesigner.ui.chat.ChatFragment"
        android:label="@string/title_chat"
        tools:layout="@layout/fragment_chat" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.esolution.vastrafashiondesigner.ui.profile.ProfileFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_profile" />
